[ "html",
  [ "p",
    "This is a paragraph with a ",
    [ "span", {"class" : "term"}, "defined term" ],
    ", but no definition." ],
  [ "p",
    "This is a paragraph with a ",
    [ "span", {
      "class": "term definition",
      "data-definition": "a term that has been defined"
      }, "defined term" ],
    " and included definition." ],
  [ "p", "And another paragraph." ] ]
